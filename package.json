    "@angular/router": "^15.1.0",
    "@kasso/weather": "^1.0.21",
    "lodash": "^4.17.21",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "verdaccio": "^5.23.2",
    "zone.js": "~0.12.0"
  },
