{
  "name": "weatherwidget",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build weather",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "publish": "ng build weather && cd dist/weather && npm publish --registry http://localhost:4873",
